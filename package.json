    "mobx-react-router": "^4.0.5",
    "object-hash": "^1.3.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
