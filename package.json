{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "yarn clean && yarn lint && yarn webpack:prod",
    "clean:dist": "rm -rf dist",
    "clean": "yarn clean:dist && yarn",
    "format:css": "stylelint --fix src/**/*.less",
    "format:js": "eslint --ext .js --ext .jsx --fix src",
    "lint:css": "stylelint src/**/*.less",
    "lint:js": "eslint --ext .js --ext .jsx src",
    "lint": "yarn lint:js && yarn lint:css",
    "start": "yarn clean && yarn webpack:dev",
    "webpack:dev": "NODE_ENV=development API_ENV=development webpack-dev-server --config webpack.config.js --progress --colors --content-base dist/ --hot --inline --history-api-fallback --host 0.0.0.0",
    "webpack:prod": "webpack --config webpack.config.js"
  },
  "license": "MIT",
  "dependencies": {
    "antd": "^3.4.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-christianrank": "^3.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eventemitter2": "^5.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "history": "^4.7.2",
    "html-minifier": "^3.5.13",
    "html-webpack-plugin": "^3.2.0",
    "jsx-loader": "^0.13.2",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "normalize.css": "^8.0.0",
    "object-fill": "^1.0.4",
    "object-hash": "^1.3.0",
    "postcss-loader": "^2.1.3",
    "react": "^16.3.1",
    "react-css-modules": "^4.7.2",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-localstorage": "rc",
    "redux-localstorage-filter": "^0.1.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.3.0",
    "style-loader": "^0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "uglify-js": "^3.3.19",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^2.11.1"
  }
}
