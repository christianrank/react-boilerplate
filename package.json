{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean:dist": "cd dist; rm -rf `ls | grep -vE 'cdn|img'` | :",
    "clean:src": "cd src; rm -rf messages",
    "clean": "yarn run clean:dist && yarn run clean:src",
    "format:js": "eslint --fix src",
    "format:css": "stylefmt -c stylelint.config.js -r src/**/*.scss",
    "format:css:dry": "stylefmt -c stylelint.config.js -d -r src/**/*.scss",
    "_temporary_disabled_css_format:all": "yarn run format:js && yarn run format:css",
    "format:all": "yarn run format:js",
    "lint:js": "eslint src",
    "lint:css": "stylelint src/**/*.scss",
    "lint:all": "yarn run lint:js && yarn run lint:css",
    "build:js": "NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "_todo_build:lang": "babel-node --presets es2015 src/js/translate.js",
    "_todo_build:all": "yarn run build:js && yarn run build:lang",
    "build:all": "yarn run build:js",
    "watch:js": "NODE_ENV=development webpack-dev-server --config webpack.config.js --progress --colors --content-base dist/ --hot --inline --history-api-fallback --host 0.0.0.0",
    "watch:all": "yarn run watch:js",
    "min:js": "uglifyjs ./dist/app.js --compress --mangle -o ./dist/app.js",
    "min:css": "uglifycss --cute-comments ./dist/app.css > ./dist/app.tmp.css && mv -f ./dist/app.tmp.css ./dist/app.css",
    "min:all": "yarn run min:js && yarn run min:css",
    "build": "yarn run clean && yarn run format:all && yarn run lint:all && yarn run build:all && yarn run min:all",
    "watch": "yarn run clean && yarn run watch:all",
    "release:docker:build": "docker build -t git.bitrain.co:4567/PROJECT_NAME/web .",
    "release:docker:push": "docker push git.bitrain.co:4567/PROJECT_NAME/web",
    "release:docker": "yarn run release:docker:build; yarn run release:docker:push",
    "release": "yarn run build; yarn run release:docker"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-christianrank": "^1.0.7",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "eventemitter2": "^4.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "history": "^4.6.1",
    "html-minifier": "^3.5.2",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.2.1",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "object-fill": "^1.0.4",
    "object-hash": "^1.1.8",
    "postcss-loader": "^2.0.5",
    "react": "^15.5.4",
    "react-css-modules": "^4.3.0",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-localstorage": "rc",
    "redux-localstorage-filter": "^0.1.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "stylefmt": "^6.0.0",
    "stylelint": "^7.11.0",
    "stylelint-config-standard": "^16.0.0",
    "uglify-js": "^3.0.15",
    "uglifycss": "^0.0.27",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
