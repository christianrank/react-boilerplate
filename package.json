{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean:dist": "cd dist; rm -rf `ls | grep -vE 'cdn|img'` | :",
    "clean:src": "cd src; rm -rf messages",
    "clean": "npm run clean:dist && npm run clean:src",
    "test:stylelint": "stylelint src/**/*.less",
    "test:eslint": "eslint src",
    "test": "npm run test:stylelint && npm run test:eslint",
    "build:html": "cp -f ./src/index.dev.html ./dist/index.html",
    "build:js": "NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "_todo_build:lang": "babel-node --presets es2015 src/js/translate.js",
    "_todo_build:all": "npm run build:html && npm run build:js && npm run build:lang",
    "build:all": "npm run build:html && npm run build:js",
    "watch:js": "NODE_ENV=development webpack-dev-server --config webpack.config.js --progress --colors --content-base dist/ --hot --inline --history-api-fallback --host 0.0.0.0",
    "watch:all": "npm run watch:js",
    "min:html": "cp -f ./src/index.html ./dist/index.html",
    "min:js": "uglifyjs ./dist/app.js --compress --mangle -o ./dist/app.min.js",
    "min:css": "uglifycss --cute-comments ./dist/app.css > ./dist/app.min.css",
    "min:all": "npm run min:html && npm run min:js && npm run min:css",
    "build": "npm run clean && npm run build:all && npm run min:all",
    "watch": "npm run clean && npm run build:html && npm run watch:all"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-christianrank": "^1.0.6",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "history": "^4.5.1",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.1.1",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "normalize.css": "^5.0.0",
    "object-fill": "^1.0.1",
    "postcss-loader": "^1.2.2",
    "react": "^15.4.2",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-router": "^3.0.2",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.5",
    "uglifycss": "^0.0.25",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {}
}
