{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean:dist": "cd dist; rm -rf `ls | grep -vE 'cdn|img'` | :",
    "clean:src": "cd src; rm -rf messages",
    "clean": "yarn run clean:dist && yarn run clean:src",
    "format:js": "eslint --fix src",
    "format:css": "stylefmt -c stylelint.config.js -r src/**/*.scss",
    "format:css:dry": "stylefmt -c stylelint.config.js -d -r src/**/*.scss",
    "format:all": "yarn run format:js && yarn run format:css",
    "lint:js": "eslint src",
    "lint:css": "stylelint src/**/*.scss",
    "lint:all": "yarn run lint:js && yarn run lint:css",
    "build:html": "cp -f ./src/index.dev.html ./dist/index.html",
    "build:js": "NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "_todo_build:lang": "babel-node --presets es2015 src/js/translate.js",
    "_todo_build:all": "yarn run build:html && yarn run build:js && yarn run build:lang",
    "build:all": "yarn run build:html && yarn run build:js",
    "watch:js": "NODE_ENV=development webpack-dev-server --config webpack.config.js --progress --colors --content-base dist/ --hot --inline --history-api-fallback --host 0.0.0.0",
    "watch:all": "yarn run watch:js",
    "min:html": "cp -f ./src/index.html ./dist/index.html",
    "min:js": "uglifyjs ./dist/app.js --compress --mangle -o ./dist/app.min.js",
    "min:css": "uglifycss --cute-comments ./dist/app.css > ./dist/app.min.css",
    "min:all": "yarn run min:html && yarn run min:js && yarn run min:css",
    "build": "yarn run clean && yarn run format:all && yarn run lint:all && yarn run build:all && yarn run min:all",
    "watch": "yarn run clean && yarn run build:html && yarn run watch:all"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-christianrank": "^1.0.6",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "history": "^4.5.1",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.1.1",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.5.0",
    "normalize.css": "^5.0.0",
    "object-fill": "^1.0.1",
    "postcss-loader": "^1.3.0",
    "react": "^15.4.2",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-localstorage": "rc",
    "redux-localstorage-filter": "^0.1.1",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "stylefmt": "^5.1.2",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "uglify-js": "^2.7.5",
    "uglifycss": "^0.0.25",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {}
}
